<Window x:Class="Sample.DP.MvvM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sample.DP.MvvM"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="20">
        <TextBox Text="{Binding Operator1}" Margin="5"></TextBox>
        <TextBox Text="{Binding Operator2}" Margin="5"></TextBox>
        <TextBlock Text="{Binding Result}" Margin="5"></TextBlock>
    </StackPanel>
</Window>
